{
  "root": "packages/immer",
  "sourceRoot": "packages/immer/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/immer",
        "main": "packages/immer/src/index.ts",
        "tsConfig": "packages/immer/tsconfig.lib.json",
        "assets": ["packages/immer/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/immer/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/immer"],
      "options": {
        "jestConfig": "packages/immer/jest.config.js",
        "passWithNoTests": true,
        "codeCoverage": true
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public",
        "noBuild": true
      }
    }
  },
  "tags": []
}
